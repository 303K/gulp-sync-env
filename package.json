{
  "name": "gulp-sync-env",
  "version": "0.0.1",
  "description": "A plugin for Gulp",
  "keywords": [
    "gulpplugin",
    "laravel"
  ],
  "author": " (https://github.com/303K)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/303k/gulp-sync-env.git"
